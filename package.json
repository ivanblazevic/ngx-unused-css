{
  "name": "ngx-unused-css",
  "version": "2.2.14",
  "description": "Detect unused CSS in angular components",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=./jasmine.json",
    "test-lib": "node dist/index.js"
  },
  "keywords": [
    "angular",
    "unused",
    "css",
    "detect",
    "find"
  ],
  "author": "Ivan Blazevic",
  "license": "ISC",
  "dependencies": {
    "chalk": "4.0.0",
    "jsdom": "16.3.0",
    "meow": "^6.0.0",
    "node-sass": "5.0.0",
    "purgecss": "2.1.0",
    "table": "^5.4.4",
    "tslib": "^1.11.1"
  },
  "bin": {
    "ngx-unused-css": "./dist/index.js"
  },
  "bugs": "https://github.com/ivanblazevic/ngx-unused-css/issues",
  "homepage": "https://github.com/ivanblazevic/ngx-unused-css",
  "repository": {
    "type": "git",
    "url": "https://github.com/ivanblazevic/ngx-unused-css"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm test",
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.10",
    "@types/jsdom": "^16.2.0",
    "@types/mock-fs": "^4.10.0",
    "@types/node": "^13.11.0",
    "@types/table": "^5.0.0",
    "husky": "^4.2.3",
    "jasmine": "^3.5.0",
    "jasmine-core": "^3.4.0",
    "jasmine-node": "^3.0.0",
    "mock-fs": "^4.11.0",
    "nodemon": "^2.0.2",
    "sinon": "^9.0.2",
    "ts-mock-imports": "^1.3.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
